<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <record id="view_clearance_question_category_tree" model="ir.ui.view">
    <field name="name">Clearance Question category Tree</field>
    <field name="model">clearance.question.category</field>
    <field name="arch" type="xml">
      <tree string="">
        <field name="name" />
      </tree>
    </field>
  </record>

  <record id="view_clearance_question_category_form" model="ir.ui.view">
    <field name="name">Clearance Question category Form</field>
    <field name="model">clearance.question.category</field>
    <field name="arch" type="xml">
      <form string="">
        <sheet>
          <group>
            <group>
              <field name="name" />
            </group>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="action_clearance_question_category" model="ir.actions.act_window">
    <field name="name">Clearance Question category</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">clearance.question.category</field>
    <field name="view_mode">tree,form</field>
    <field name="help" type="html">
      <p class="oe_view_nocontent_create">
        <!-- Add Text Here -->
      </p>
      <p>
        <!-- More details about what a user can do with this object will be OK -->
      </p>
    </field>
  </record>

  <menuitem
    id="menu_clearance_question_category"
    name="Category"
    sequence="111"
    parent="menu_termination_configuration"
    action="action_clearance_question_category"
  />
</odoo>
